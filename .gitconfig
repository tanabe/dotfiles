[user]
    name = "Hideaki Tanabe"
    email = tanablog@gmail.com
[color]
	ui = true
[alias]
    st = status
    br = branch
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[init]
	defaultBranch = main
[pull]
	rebase = false
[pager]
	branch = false
[core]
	editor = nvim
